[
    {
        "display-name": "Xnxx",
        "channel": "xnxx",
        "baseurl": "https://www.xnxx.com",
        "option":{
            "rootCssQuery": ".thumb-block",
            "name": {
                "cssQuery": ".thumb-under a",
                "attr": "title"
            },
            "thumb": {
                "cssQuery": "img",
                "attr": "data-src",
                "replace":[
                    "THUMBNUM",
                    "1"
                ]
            },
            "viewCount": {
                "index":0,
                "cssQuery": ".right",
                "attr":"ownText"
            },
            "duration": {
                "index":0,
                "cssQuery": ".metadata",
                "attr":"ownText"
            },
            "url": {
                "cssQuery": "a",
                "attr": "href"
            },
            "previewThumbCount": {
                "value":"20"
            },
            "previewThumbUrl": {
                "cssQuery": "img",
                "attr": "data-src",
                "replace":[
                    "(THUMBNUM|\\d+).jpg",
                    "{index}.jpg"
                ]
            }
        }
    },
    {
        "display-name": "Samusar",
        "channel": "samusar",
        "baseurl": "https://www.samusar.com",
        "option":{
            "rootCssQuery": ".list-videos .item",
            "name": {
                "cssQuery": ".title",
                "trim":true
            },
            "thumb": {
                "cssQuery": "img",
                "attr": [
                    "data-original",
                    "data-webp"
                ]
            },
            "viewCount": {
                "cssQuery": ".views"
            },
            "duration": {
                "cssQuery": ".duration"
            },
            "url": {
                "cssQuery": "a",
                "attr": "href"
            },
           "previewVideoUrl":{
            "cssQuery":"img",
            "attr":"data-preview"
           },
           "date":{
            "cssQuery":".added"
           }
        }
    },
    {
        "display-name": "Xvideos",
        "channel": "xvideos",
        "baseurl": "https://www.xvideos.com",
        "option":{
            "rootCssQuery": ".thumb-block",
            "name": {
                "cssQuery": ".thumb-under a",
                "attr": "title"
            },
            "thumb": {
                "cssQuery": "img",
                "attr": "data-src",
                "replace":[
                    "THUMBNUM",
                    "1"
                ]
            },
            "viewCount": {
                "index":0,
                "cssQuery": ".metadata",
                "attr":"ownText"
            },
            "duration": {
                "index":0,
                "cssQuery": ".duration",
                "attr":"ownText"
            },
            "url": {
                "cssQuery": "a",
                "attr": "href"
            },
            "previewThumbCount": {
                "value":"20"
            },
            "previewThumbUrl": {
                "cssQuery": "img",
                "attr": "data-src",
                "replace":[
                    "(THUMBNUM|\\d+).jpg",
                    "{index}.jpg"
                ]
            }
        }
    },
    {
        "display-name": "Eporner",
        "channel": "eporner",
        "baseurl": "https://www.eporner.com",
        "option":{
            "rootCssQuery": ".mb",
            "name": {
                "cssQuery": "img",
                "attr": "alt"
            },
             "thumb": {
                "cssQuery": "img",
                "attr": {
                    "attrs":[
                        "src",
                    "data-src"
                    ],
                    "regex":"^http"
                }
            },
            "viewCount": {
                "cssQuery": ".mbvie"
            },
            "duration": {
                "cssQuery": ".mbtim"
            },
            "url": {
                "cssQuery": "a",
                "attr": "href"
            },
            "previewVideoUrl":{
                "cssQuery": "img",
                "attr": "data-src",
                "replace":[
                    "/([^/]+)/[^/]+$",
                    "/{matched}/{matched}-preview.webm"
                ]
            }
        }
    }
]
